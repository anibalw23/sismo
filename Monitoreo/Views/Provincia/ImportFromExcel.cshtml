@model IEnumerable<Monitoreo.Models.Provincia>
@{
    ViewBag.Title = "Import Provincias From Excel";
}

<div class="panel-heading">
    <h3>@ViewBag.Title</h3>
</div>


<div class="panel-body">
    <div class=" panel-group">
        @using (Html.BeginForm("ImportFromExcelFile", "Provincia", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <input name="uploadFile" type="file" /><input type="submit" value="UploadFile" title="Upload file" onclick="return CheckFileName();" />
        }
    </div>

    <div class="panel-group">
        
        <h3> Formato del archivo de Excel..</h3>
        <table>
            <tr>
                <th>Codigo</th>
                <th>Provincia</th>
            </tr>
            <tr>
                <td>001</td>
                <td>Santo Domingo</td>
            </tr>
            <tr>
                <td>002</td>
                <td>Santago</td>
            </tr>
            <tr>
                <td>nnn</td>
                <td>ect</td>
            </tr>


        </table>
        
         
    </div>


</div>
